<odoo>
    <record id="view_import_so_lines_wizard" model="ir.ui.view">
        <field name="name">import.so.lines.wizard.form</field>
        <field name="model">import.so.lines.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Sale Order Lines" >
                <group>
                    <field name="file" filename="File"/>
                </group>
                <footer>
                    <button name="import_so_lines" string="Import" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
    
</odoo>
